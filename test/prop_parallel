CRIT11=`$GREP "11 * 1226\.49 *  20\.750 * \-*0*\.000" $log | wc -l`
CRIT12=`$GREP "12 * 1185\.79 *  0*\.000 * \-*0*\.000" $log | wc -l`
CRIT13=`$GREP "13 *  965\.66 *  22\.51[12] * \-*0*\.000" $log | wc -l`
CRIT14=`$GREP "14 *  960\.4[23] *   7\.089 * \-*0*\.000" $log | wc -l`
CRIT15=`$GREP "15 *  957\.42 * 417\.46. * \-*0*\.000" $log | wc -l`
CRIT16=`$GREP " 1 * 3259\.27 *  72\.56. *   27\.23." $log | wc -l`
CRIT17=`$GREP " 2 * 3256\.7[12] *5\.841 * \-22\.11." $log | wc -l`
CRIT18=`$GREP " 3 * 2406\.34 *   7\.064 *   11\.28." $log | wc -l`
CRIT19=`$GREP " 4 * 2384\.28 *  60\.222 * \-12\.27." $log | wc -l`
CRIT20=`$GREP " 5 * 1577\.60 *   7\.506 * \-10\.83." $log | wc -l`
CRIT21=`$GREP " 6 * 1503\.69 *   1\.98. *  \-2\.32." $log | wc -l`
