CRIT12=`$GREP "12 * A * 940\.4[0-9] * (0| )\.00428[0-9] * 66\.24[0-9] * 1\.567[0-9]" $log | wc -l`
CRIT13=`$GREP "13 * A * 741\.0[0-9] * (0| )\.00337[0-9] * 9\.04[0-9] * (0| )\.214[0-9]" $log | wc -l`
