CTRL[21]=10
ERROR[21]="CIRCULAR INTENSITY DIFFERENCES FREQ3 NOT CORRECT"

# Isotopic frequencies
CRIT1=`$GREP "1 * A * 3381\.7[0-9] * (0| )\.01540[0-9] * 5\.64[0-9] * (0| )\.133[0-9]" $log | wc -l`
