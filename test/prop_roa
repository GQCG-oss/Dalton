CRIT6=`$GREP "Final * HF energy\: * \-115\.8302822118[0-9][0-9]" $log | wc -l`
CRIT7=`$GREP "Final * HF energy\: * \-115\.8302822897[0-9][0-9]" $log | wc -l`
