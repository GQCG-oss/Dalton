#!/bin/sh
#
# This is the script for generating files for a specific Dalton test job.
#
# For the .check file ksh or bash is preferred, otherwise use sh
# (and hope it is not the old Bourne shell, which will not work)
#
if [ -x /bin/ksh ]; then
   CHECK_SHELL='#!/bin/ksh'
elif [ -x /bin/bash ]; then
   CHECK_SHELL='#!/bin/bash'
else
   CHECK_SHELL='#!/bin/sh'
fi


#######################################################################
#  TEST DESCRIPTION
#######################################################################
cat > prop_exci_aorpa.info <<'%EOF%'
   prop_exci_aorpa
   ---------------
   Molecule:         H2O
   Wave Function:    SCF (AO-RPA through AO-SOPPA module)
   Test Purpose:     Calculation of excitation energies through ABACUS
%EOF%

#######################################################################
#  MOLECULE INPUT
#######################################################################
cat > prop_exci_aorpa.mol <<'%EOF%'
INTGRL
Calculation of excitation energies through ABACUS

c   2    2  X  Y      0.10D-14
      8.00    1    3    6    5    1
O   0.00000             0.00000              0.1241444240
F  6    1
31195.6 0.00021
4669.38 0.001628
1062.62 0.00845
301.426 0.034191
98.5153 0.110311
35.4609 .269493
F  1    1
13.6179 .423545
F  1    1
5.38618 .283038
F  1    1
1.53873 .027484
F  1    1
.6055 -.005144
F  1    1
.22054 .00164
F  3    1
114.863 .002266
26.8767 0.017192
8.32077 0.075341
F  1    1
2.97237 .212775
F  1    1
1.12848 .370478
F  1    1
.4236 .398523
F  1    1
.15074 .184703
F  2    1
1.21879 .357851
.36102 .759561
      1.00    1    2    3    1
H   0.00000             1.43153              -0.985265576
F  3    1
33.865014 .006068
5.094788 0.045316
1.158786 0.202846
F  1    1
0.32584 0.503709
F  1    1
.102741 .383568
F  1    1
1.16 1.0
%EOF%

#######################################################################
#  DALTON INPUT
#######################################################################
cat > prop_exci_aorpa.dal <<'%EOF%'
**DALTON INPUT
.RUN PROPERTIES
**WAVE FUNCTIONS
.HF
**PROPERTIES
.SOPPA
.EXCITA
.PRINT
 2
*SOPPA
.AORPA
*EXCITA
.DIPSTR
.ROTVEL
.NEXCITA
    5    5    5    5    0    0    0    0
**END OF DALTON INPUT
%EOF%
#######################################################################



#######################################################################

#######################################################################
#  CHECK SCRIPT
#######################################################################
echo $CHECK_SHELL >prop_exci_aorpa.check
cat >>prop_exci_aorpa.check <<'%EOF%'
log=$1

if [ `uname` = Linux ]; then
   GREP="egrep -a"
else
   GREP="egrep"
fi

# Basis set
CRIT1=`$GREP "O * 1 * 8.0000 * 44 * 27 * \[11s7p2d\|6s5p1d\]" $log | wc -l`
CRIT2=`$GREP "H * 2 * 1.0000 * 8 * 6 * \[5s1p\|3s1p\]" $log | wc -l`
CRIT3=`$GREP "total\: * 3 * 10.0000 * 60 * 39" $log | wc -l`
CRIT4=`$GREP "Cartesian basis used" $log | wc -l`
TEST[1]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4`
CTRL[1]=4
ERROR[1]="BASIS SET NOT READ CORRECTLY"

# Geometry
CRIT1=`$GREP "Total number of coordinates\: * 9" $log | wc -l`
CRIT2=`$GREP "O * \: * 1 * x * (0| )\.0000000000 * 2 * y * (0| )\.0000000000 * 3 * z * (0| )\.1241444240" $log | wc -l`
CRIT3=`$GREP "H * \/ 1 \: * 4 * x * (0| )\.0000000000 * 5 * y * 1\.4315300000 * 6 * z * (\-0| \-)\.9852655760" $log | wc -l`
CRIT4=`$GREP "H * \/ 2 \: * 7 * x * (0| )\.0000000000 * 8 * y * \-1\.4315300000 * 9 * z * (\-0| \-)\.9852655760" $log | wc -l`
TEST[2]=`expr   $CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4`
CTRL[2]=4
ERROR[2]="GEOMETRY NOT READ CORRECTLY"

# Symmetry
CRIT1=`$GREP "Number of coordinates in each symmetry\: * 3 * 2 * 3 * 1" $log | wc -l`
CRIT2=`$GREP "Number of orbitals in each symmetry\: * 19 * 7 * 11 * 2" $log | wc -l`
TEST[3]=`expr	$CRIT1 \+ $CRIT2`
CTRL[3]=2
ERROR[3]="SYMMETRY NOT CORRECT"

# Energies
CRIT1=`$GREP "Final * HF * energy\: * \-76\.060784218[0-9]" $log | wc -l`
TEST[4]=`expr	$CRIT1 `
CTRL[4]=1
ERROR[4]="ENERGY NOT CORRECT"

# Property calculation
CRIT1=`$GREP "Electronic excitation energies" $log | wc -l`
CRIT2=`$GREP "AO integral driven RPA" $log | wc -l`
CRIT3=`$GREP "Number of excitation energies\: * 5 * 5 * 5 * 5 * 0 * 0 * 0 * 0" $log | wc -l`
CRIT4=`$GREP "Conventional rotational strength" $log | wc -l`
CRIT5=`$GREP "Dipole strength" $log | wc -l`
TEST[5]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5`
CTRL[5]=5
ERROR[5]="CALCULATION OF PROPERTIES NOT SET UP CORRECTLY"

# RPA Electric transition dipole moments
