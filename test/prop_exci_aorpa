CRIT4=`$GREP "1 * 4 * 0*\.78081[0-9] * \-*0*\.0000 * \-*0*\.0000 * \-*0*\.017[0-9] * \-*0*\.0000 * \-*0*\.0000 * \-*0*\.007[0-9]" $log | wc -l`
CRIT5=`$GREP "1 * 5 * 0*\.83882[0-9] * \-*0*\.0000 * \-*0*\.0000 * \-*0*\.022[0-9] * \-*0*\.0000 * \-*0*\.0000 * \-*0*\.004[0-9]" $log | wc -l`
CRIT6=`$GREP "2 * 1 * 0*\.332(09|10)[0-9] * \-*0*\.520[0-9] * \-*0*\.0000 * \-*0*\.0000 * \-*0*\.462[0-9] * \-*0*\.0000 * \-*0*\.0000" $log | wc -l`
CRIT7=`$GREP "2 * 2 * 0*\.61175[0-9] * \-*0*\.118[0-9] * \-*0*\.0000 * \-*0*\.0000 * \-*0*\.184[0-9] * \-*0*\.0000 * \-*0*\.0000" $log | wc -l`
