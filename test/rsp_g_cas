#!/bin/sh
#
# This is the script for generating files for a specific Dalton test job.
#
# For the .check file ksh or bash is preferred, otherwise use sh
# (and hope it is not the old Bourne shell, which will not work)
#
if [ -e /bin/ksh ]; then
   CHECK_SHELL='#!/bin/ksh'
elif [ -e /bin/bash ]; then
   CHECK_SHELL='#!/bin/bash'
else
   CHECK_SHELL='#!/bin/sh'
fi


#######################################################################
#  TEST DESCRIPTION
#######################################################################
cat << EODAL > rsp_g_cas.dal
**DALTON INPUT
.RUN RESPONSE
**INTEGRALS
.SPIN-ORBIT
**WAVE FUNCTIONS
.HF
.MCSCF
*SCF INPUT
.DOUBLY OCC
 3 1 1 0 2 1 0 0
*CONFIGURATION INPUT
.SYMMETRY
 4
.SPIN MULT
 3
.INACTIVE SPACE
 3 1 1 0 2 0 0 0
.ELECTRONS
 2
.CAS SPACE
 0 0 0 0 0 1 1 0
*OPTIMIZATION
.DETERMINANTS
**RESPONSE
*ESR
.G-TENSOR
.OZSO1
.OZSO2
*SPIN-ORBIT
*END OF INPUT
EODAL
cat << EOMOL > rsp_g_cas.mol
ATOMBASIS
Calculation of linear response spin-orbit

C   1    3  X  Y  Z     1.D-12
        8.    1 Basis=ano-1 3  2  1  0
O      0.00000   0.00000   1.14050
O      0.00000   0.00000  -1.14050
EOMOL
cat << EOREF > rsp_g_cas.ref


