cat << 'EOCHK' > rsp_g_cas.check
log=$1
GREP=egrep

# ?
CRIT1=`$GREP "Final MCSCF energy\: * \-149\.649025434184" $log | wc -l`
TEST[1]=`expr	$CRIT1`
CTRL[1]=1
ERROR[1]="ENERGY NOT CORRECT"

# ?
CRIT1=`$GREP "\@G * _\|_ * \|\|" $log | wc -l`
CRIT2=`$GREP "\@G OZ\-SO1 * 5226\. *[- ]0\." $log | wc -l`
CRIT3=`$GREP "\@G OZ\-SO2 \-1586\. *[- ]0\." $log | wc -l`
CRIT4=`$GREP "\@G Total * 3640\. *[- ]0\." $log | wc -l`
TEST[2]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4`
CTRL[2]=4
ERROR[2]="G-TENSOR NOT CORRECT"
str="1 2"
PASSED=1
for i in $str
do
   if [ ${TEST[i]} -ne ${CTRL[i]} ]; then
     echo ${ERROR[i]}
     PASSED=0
   fi
done

if [ $PASSED -eq 1 ]
then
  echo TEST ENDED PROPERLY
  exit 0
else
  echo THERE IS A PROBLEM
  exit 1
fi
EOCHK
