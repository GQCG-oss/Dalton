CRIT16=`$GREP "4 * 1 * 10\.747[0-9] * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.092[0-9][0-9] * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000" $log | wc -l`
