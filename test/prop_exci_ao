#echo "RPA MAGNETIC TRANSITION DIPOLE MOMENTS"
#echo $CRIT1 $CRIT2 $CRIT3 $CRIT4 $CRIT5 $CRIT6 $CRIT7 $CRIT8 $CRIT9 $CRIT10 $CRIT11 $CRIT12 $CRIT13 $CRIT14 $CRIT15 $CRIT16 $CRIT17 $CRIT18 $CRIT19 $CRIT20
CTRL[7]=20
ERROR[7]="RPA MAGNETIC TRANSITION DIPOLE MOMENTS NOT CORRECT"

# RPA Oscillator and Rotational Strengths
CRIT1=`$GREP "1 * 1 * 11\.532[0-9] * \-*0*\.136[0-9] * \-*0*\.126[0-9] * \-*0*\.000 * \-*0*\.000 * \-*0*\.000" $log | wc -l`
