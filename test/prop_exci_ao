CRIT10=`$GREP "2 * 5 * 29\.059[0-9] * \-*0*\.00000 * \-*0*\.023[0-9][0-9] * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000" $log | wc -l`
