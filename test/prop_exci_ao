CRIT16=`$GREP "4 * 1 * (0| )\.39497[0-9] * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
CRIT17=`$GREP "4 * 2 * (0| )\.65410[0-9] * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
CRIT18=`$GREP "4 * 3 * (0| )\.78672[0-9] * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
CRIT19=`$GREP "4 * 4 * (0| )\.83047[0-9] * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
CRIT20=`$GREP "4 * 5 * 1\.34957[0-9] * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
TEST[6]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6 \+ \
		$CRIT7 \+ $CRIT8 \+ $CRIT9 \+ $CRIT10 \+ $CRIT11 \+ $CRIT12 \+ \
		$CRIT13 \+ $CRIT14 \+ $CRIT15 \+ $CRIT16 \+ $CRIT17 \+ $CRIT18 \+ \
		$CRIT19 \+ $CRIT20`
#echo "RPA ELECTRIC TRANSITION DIPOLE MOMENTS"
#echo $CRIT1 $CRIT2 $CRIT3 $CRIT4 $CRIT5 $CRIT6 $CRIT7 $CRIT8 $CRIT9 $CRIT10 $CRIT11 $CRIT12 $CRIT13 $CRIT14 $CRIT15 $CRIT16 $CRIT17 $CRIT18 $CRIT19 $CRIT20
CTRL[6]=20
ERROR[6]="RPA ELECTRIC TRANSITION DIPOLE MOMENTS NOT CORRECT"

# RPA Magnetic transition dipole moments
CRIT1=`$GREP "1 * 1 * 11\.532[0-9]  * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000" $log | wc -l`
