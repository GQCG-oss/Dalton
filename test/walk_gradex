# Second energy
CRIT1=`$GREP "Final * HF energy\: * \-56\.19077735967[0-9]" $log | wc -l`
TEST[10]=`expr	$CRIT1`
CTRL[10]=1
ERROR[10]="SECOND ENERGY NOT CORRECT"

# Second gradient
CRIT1=`$GREP "N * (\-0|\-)\.000460502[0-9] * (0| )\.000000427[0-9] * (\-0|\-)\.008837004[0-9]" $log | wc -l`
