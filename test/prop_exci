CRIT3=`$GREP "No Darwin correction calculated" $log | wc -l`
CRIT4=`$GREP "Linear response properties" $log | wc -l`
CRIT5=`$GREP "Number of excitation energies\: * 5 * 5 * 5 * 5 * 0 * 0 * 0 * 0" $log | wc -l`
CRIT6=`$GREP "Conventional rotational strength" $log | wc -l`
CRIT7=`$GREP "Dipole strength" $log | wc -l`
CRIT8=`$GREP "Number of frequencies * \: * 3" $log | wc -l`
CRIT9=`$GREP "Frequencies * \: * (0| )\.000000 * (0| )\.050000 * (0| )\.100000" $log | wc -l`
TEST[5]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6 \+ \
		$CRIT7 \+ $CRIT8 \+ $CRIT9`
CTRL[5]=9
ERROR[5]="CALCULATION OF PROPERTIES NOT SET UP CORRECTLY"

# Polarizability tensor freq1
CRIT1=`$GREP "Ex * 6\.114547[0-9] * (0| )\.0000000 * (0| )\.0000000" $log | wc -l`
