CRIT19=`$GREP "4 * 4 * 22\.699[0-9] * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.0402[0-9] * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000" $log | wc -l`
