CRIT1=`$GREP "1 * 1 * 10\.022[0-9] * \-*0*\.152[0-9] * \-*0*\.116[0-9] * \-*0*\.000 * \-*0*\.000 * \-*0*\.000" $log | wc -l`
CRIT2=`$GREP "1 * 2 * 16\.059[0-9] * \-*0*\.054[0-9] * \-*0*\.053[0-9] * \-*0*\.000 * \-*0*\.000 * \-*0*\.000" $log | wc -l`
