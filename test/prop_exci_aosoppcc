CRIT19=`$GREP "4 * 4 * (0| )\.83418[0-9] * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
CRIT20=`$GREP "4 * 5 * 1\.35155[0-9] * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000 * (0| )\.0000" $log | wc -l`
TEST[6]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6 \+ \
		$CRIT7 \+ $CRIT8 \+ $CRIT9 \+ $CRIT10 \+ $CRIT11 \+ $CRIT12 \+ \
		$CRIT13 \+ $CRIT14 \+ $CRIT15 \+ $CRIT16 \+ $CRIT17 \+ $CRIT18 \+ \
		$CRIT19 \+ $CRIT20`
CTRL[6]=20
ERROR[6]="SOPPA(CCSD) ELECTRIC TRANSITION DIPOLE MOMENTS NOT CORRECT"
#echo "SOPPA(CCSD) ELECTRIC TRANSITION DIPOLE MOMENTS"
#echo $CRIT1 $CRIT2 $CRIT3 $CRIT4 $CRIT5 $CRIT6 $CRIT7 $CRIT8 $CRIT9 $CRIT10 $CRIT11 $CRIT12 $CRIT13 $CRIT14 $CRIT15 $CRIT16 $CRIT17 $CRIT18 $CRIT19 $CRIT20

# SOPPA(CCSD) Magnetic transition dipole moments
CRIT1=`$GREP "1 * 1 * 10\.022[0-9] * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000 * \-*0*\.00000" $log | wc -l`
