#!/bin/ksh
log=$1

if [ `uname` = Linux ]; then
   GREP="egrep -a"
else
   GREP="egrep"
fi

# gets the number of failed Gen1Int tests
NUM_FAILED=`$GREP "Gen1Int failed" $log | wc -l`

if [ $NUM_FAILED -eq 0 ]
then
  NUM_ERROR=`$GREP "ERROR" $log | wc -l`
  if [ $NUM_ERROR -eq 0 ]
  then
    echo TEST ENDED PROPERLY
    exit 0
  else
    echo THERE IS A PROBLEM 
    exit 1
  fi
else
  echo THERE IS A PROBLEM 
  exit 1
fi
