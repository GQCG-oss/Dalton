TEST[13]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6 \+ \
		$CRIT7`
CTRL[13]=7
ERROR[13]="MOLECULAR G-TENSOR NOT CORRECT"

# Shielding tensor
 CRIT1=`$GREP   "C * x * 95\.685..... * \-*0*\.000..... * \-*0*\.000....." $log | wc -l`
 CRIT2=`$GREP   "C * y * \-*0*\.000..... * 95\.685..... * \-*0*\.000....." $log | wc -l`
 CRIT3=`$GREP   "C * z * \-*0*\.000..... * \-*0*\.000..... * 191\.107....." $log | wc -l`
 CRIT4=`$GREP   "F * x * 515\.174..... * \-*0*\.000..... * \-*0*\.000....." $log | wc -l`
 CRIT5=`$GREP   "F * y * \-*0*\.000..... * 515\.174..... * \-*0*\.000....." $log | wc -l`
