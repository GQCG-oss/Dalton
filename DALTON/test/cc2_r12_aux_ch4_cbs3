#######################################################################
#  DALTON INPUT
#######################################################################
cat > cc2_r12_aux_ch4_cbs3.dal <<'%EOF%'
**DALTON INPUT
.RUN WAVE FUNCTION
*MOLBAS
.R12AUX
**INTEGRALS
.R12
.DIPLEN
**WAVE FUNCTION
.CC
*ORBITAL INPUT
.MOSTART
 H1DIAG
.R12ORB
 1 1 1 1
*SCF IN
.THRESH
 1.0D-10
*CC INPUT
.CC2
.CCSD
.PRINT
 5
.FREEZE
 1 0
.THRENR
 1.0D-10
.THRLEQ
 1.0D-08
.MAX IT
 100
*CCLR
.DIPOLE
*CCQR
.DIPOLE
*CCCR
.DIPOLE
*R12 INPUT
.NO HYB
.CC2
 1B
.NATVIR
.CABS
**END OF DALTON INPUT
%EOF%

#######################################################################

 

#######################################################################

#######################################################################
#  CHECK SCRIPT
#######################################################################
