CRIT14=`$GREPL "2 * (\-0| \-)\.1859448[0-9] * (\-0| \-)\.1499232[0-9] * 0*\.1207049[0-9] * (\-0| \-)\.1859448[0-9]"  $log | wc -l`
CRIT15=`$GREPL "3 * (\-0| \-)\.1499231[0-9] * (\-0| \-)\.3590613[0-9] * (\-0| \-)\.2090671[0-9] * 0*\.1499231[0-9]"  $log | wc -l`
CRIT16=`$GREPL "4 * (\-0| \-)\.0879808[0-9] * (\-0| \-)\.1523873[0-9] * 0*\.0002226[0-9] * (\-0| \-)\.0112991[0-9]"  $log | wc -l`
CRIT17=`$GREPL "5 * 0*\.1868274[0-9] * 0*\.1690357[0-9] * (\-0| \-)\.0112991[0-9] * 0*\.0161106[0-9]"  $log | wc -l`
CRIT18=`$GREPL "6 * 0*\.1690357[0-9] * 0*\.3820131[0-9] * 0*\.0312629[0-9] * (\-0| \-)\.0294765[0-9]"  $log | wc -l`
