#
# This is the script for generating files for a specific Dalton test job.
#
# For the .check file ksh or bash is preferred, otherwise use sh
# (and hope it is not the old Bourne shell, which will not work)
#
if [ -x /bin/ksh ]; then
   CHECK_SHELL='#!/bin/ksh'
elif [ -x /bin/bash ]; then
   CHECK_SHELL='#!/bin/bash'
else
   CHECK_SHELL='#!/bin/sh'
fi


#######################################################################
#  TEST DESCRIPTION
#######################################################################
cat > walk_vibave.info <<'%EOF%'
   walk_vibave
   -----------
   Molecule:         NH3
   Wave Function:    SCF / STO-3G
   Test Purpose:     Vibrational average of shieldings, magnetizabilities
                     and polarizabilities
%EOF%

#######################################################################
#  MOLECULE INPUT
#######################################################################
cat > walk_vibave.mol <<'%EOF%'
BASIS
STO-3G
Vibrational averaging of shieldings, magnetizabilities and polarizabilities

Atomtypes=2 Nosymmetry
Charge=7.0 Atoms=1
N          0.6023400            0.00000000              0.00000000
Charge=1.0 Atoms=3
H         -0.1947831            0.899791                1.558484
H         -0.1947831            0.899791               -1.558484
H         -0.1947831           -1.7995826090           -0.000000
%EOF%

#######################################################################
#  DALTON INPUT
#######################################################################
cat > walk_vibave.dal <<'%EOF%'
**DALTON INPUT
.WALK
*WALK
.NOORTH
.VIBAVE
.DISPLAC
 0.05
.ECKART
N          0.6037353032            0.0000000000            0.0000000000
H         -0.2012451011            0.8886984766            1.5392709141
H         -0.2012451011            0.8886984766           -1.5392709141
H         -0.2012451011           -1.7773969533            0.0000000000
**WAVE FUNCTIONS
.HF
*SCF INPUT
.THRESH
2.0D-9
**START
.SHIELD
.MAGNET
.POLARI
*LINRES
.THRESH
1.0D-7
**EACH STEP
.PRINT
 0
.SHIELD
.MAGNET
.POLARI
*LINRES
.THRESH
1.0D-7
**END OF DALTON INPUT
%EOF%
#######################################################################



#######################################################################

#######################################################################
#  CHECK SCRIPT
#######################################################################
echo $CHECK_SHELL >walk_vibave.check
cat >>walk_vibave.check <<'%EOF%'
log=$1

if [ `uname` = Linux ]; then
   GREPL="egrep -a -l"
else
   GREPL="egrep -l"
fi

# Basis set
CRIT1=`$GREPL "Basis set used is \"STO\-3G\" from the basis set library\." $log | wc -l`
CRIT2=`$GREPL "N * 1 * 7\.0000 * 15 * 5 * \[6s3p\|2s1p\]" $log | wc -l`
CRIT3=`$GREPL "H * 3 * 1\.0000 * 3 * 1 * \[3s\|1s\]" $log | wc -l`
CRIT4=`$GREPL "total\: * 4 * 10\.0000 * 24 * 8" $log | wc -l`
TEST[1]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4`
CTRL[1]=4
ERROR[1]="BASIS SET NOT READ CORRECTLY"

# Geometry
CRIT1=`$GREPL "Total number of coordinates\: * 12" $log | wc -l`
CRIT2=`$GREPL "1 * x * 0*\.6023400000" $log | wc -l`
CRIT3=`$GREPL "2 * y * (\-0|\-|0| )\.0000000000" $log | wc -l`
CRIT4=`$GREPL "3 * z * (\-0|\-|0| )\.0000000000" $log | wc -l`
CRIT5=`$GREPL "4 * x * (\-0| \-)\.1947831000" $log | wc -l`
CRIT6=`$GREPL "5 * y * 0*\.8997910000" $log | wc -l`
CRIT7=`$GREPL "6 * z * 1\.5584840000" $log | wc -l`
CRIT8=`$GREPL "7 * x * (\-0| \-)\.1947831000" $log | wc -l`
CRIT9=`$GREPL "8 * y * 0*\.8997910000" $log | wc -l`
CRIT10=`$GREPL "9 * z * \-1\.5584840000" $log | wc -l`
CRIT11=`$GREPL "10 * x * (\-0| \-)\.1947831000" $log | wc -l`
CRIT12=`$GREPL "11 * y * \-1\.7995826090" $log | wc -l`
CRIT13=`$GREPL "12 * z * (\-0|\-|0| )\.0000000000" $log | wc -l`
TEST[2]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6 \+ \
		$CRIT7 \+ $CRIT8 \+ $CRIT9 \+ $CRIT10 \+ $CRIT11 \+ $CRIT12 \+ \
		$CRIT13`
CTRL[2]=13
ERROR[2]="GEOMETRY NOT READ CORRECTLY"

# Initial energy
CRIT1=`$GREPL "Final * HF energy\: * \-55\.4551193324[0-9][0-9]" $log | wc -l`
TEST[3]=`expr	$CRIT1`
CTRL[3]=1
ERROR[3]="INITIAL ENERGY NOT CORRECT"

# Numerical differentiation
CRIT1=`$GREPL "Walk type\: Numerical differentiation" $log | wc -l`
CRIT2=`$GREPL "Vibrational averaging of * molecular properties at an effective geometry" $log | wc -l`
CRIT3=`$GREPL "Molecular properties transformed to the following Eckart frame" $log | wc -l`
CRIT4=`$GREPL "N * 0*\.6037353032 * (\-0|\-|0| )\.0000000000 * (\-0|\-|0| )\.0000000000" $log | wc -l`
CRIT5=`$GREPL "H * (\-0| \-)\.2012451011 * 0*\.8886984766 * 1\.5392709141" $log | wc -l`
CRIT6=`$GREPL "H * (\-0| \-)\.2012451011 * 0*\.8886984766 * \-1\.5392709141" $log | wc -l`
CRIT7=`$GREPL "H * (\-0| \-)\.2012451011 * \-1\.7773969533 * (\-0|\-|0| )\.0000000000" $log | wc -l`
TEST[4]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6 \+ \
		$CRIT7`
CTRL[4]=7
ERROR[4]="NUMERICAL DIFFERENTIATION NOT SET UP CORRECTLY"

# Inital gradient
CRIT1=`$GREPL "N * 0*\.00666549[0-9]* * 0*\.000000190[0-9] * (0| |\-0|\-)\.000000000[0-9]" $log | wc -l`
CRIT2=`$GREPL "H * (\-0| \-)\.00222180(3|4)[0-9] * 0*\.00488237(6|7)[0-9] * 0*\.008456546[0-9]" $log | wc -l`
CRIT3=`$GREPL "H * (\-0| \-)\.00222180(3|4)[0-9] * 0*\.00488237(6|7)[0-9] * (\-0| \-)\.008456546[0-9]" $log | wc -l`
CRIT4=`$GREPL "H * (\-0| \-)\.002221887[0-9] * (\-0| \-)\.00976494(3|4)[0-9] * (\-0|\-|0| )\.000000000[0-9]" $log | wc -l`
TEST[5]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4`
CTRL[5]=4
ERROR[5]="INITIAL GRADIENT NOT CORRECT"

# Initial Hessian
CRIT1=`$GREPL "N * x * 0*\.32280[0-9]" $log | wc -l`
CRIT2=`$GREPL "N * y * (\-0|\-|0| )\.000000 * 0*\.81860[0-9]" $log | wc -l`
CRIT3=`$GREPL "N * z * (\-0|\-|0| )\.000000 * (\-0|\-|0| )\.000000 * 0*\.81860[0-9]" $log | wc -l`
CRIT4=`$GREPL "H * x * (\-0| \-)\.10760[0-9] * 0*\.11963[0-9] * 0*\.20720[0-9] * 0*\.11013[0-9]" $log | wc -l`
CRIT5=`$GREPL "H * y * 0*\.06655[0-9] * (\-0| \-)\.18631[0-9] * (\-0| \-)\.14992[0-9] * (\-0| \-)\.08762[0-9] * 0*\.18830[0-9]" $log | wc -l`
CRIT6=`$GREPL "H * z * 0*\.11527[0-9] * (\-0| \-)\.14992[0-9] * (\-0| \-)\.35942[0-9] * (\-0| \-)\.15176[0-9] * 0*\.16825[0-9] * 0*\.38258[0-9]" $log | wc -l`
CRIT7=`$GREPL "H * x * (\-0| \-)\.10760[0-9] * 0*\.11963[0-9] * (\-0| \-)\.20720[0-9] * (\-0| \-)\.00126[0-9] * (\-0| \-)\.01094[0-9] * 0*\.03064[0-9]" $log | wc -l`
CRIT8=`$GREPL "H * y * 0*\.06655[0-9] * (\-0| \-)\.18631[0-9] * 0*\.14992[0-9] * (\-0| \-)\.01094[0-9] * 0*\.01487[0-9] * (\-0| \-)\.02869[0-9]" $log | wc -l`
CRIT9=`$GREPL "H * z * (\-0| \-)\.11527[0-9] * 0*\.14992[0-9] * (\-0| \-)\.35942[0-9] * (\-0| \-)\.03064[0-9] * 0*\.02869[0-9] * (\-0| \-)\.02745[0-9]" $log | wc -l`
CRIT10=`$GREPL "H * x * (\-0| \-)\.10760[0-9] * (\-0| \-)\.23926[0-9] * (\-0|\-|0| )\.000000 * (\-0| \-)\.00126[0-9] * 0*\.03200[0-9] * 0*\.00584[0-9]" $log | wc -l`
CRIT11=`$GREPL "H * y * (\-0| \-)\.13311[0-9] * (\-0| \-)\.44598[0-9] * (\-0|\-|0| )\.000000 * (\-0| \-)\.02106[0-9] * (\-0| \-)\.01687[0-9] * 0*\.01036[0-9]" $log | wc -l`
CRIT12=`$GREPL "H * z * (\-0|\-|0| )\.000000 * (\-0|\-|0| )\.000000 * (\-0| \-)\.09975[0-9] * (\-0| \-)\.02479[0-9] * (\-0| \-)\.04702[0-9] * 0*\.00429[0-9]" $log | wc -l`
CRIT13=`$GREPL "H * x * 0*\.11013[0-9]" $log | wc -l`
CRIT14=`$GREPL "H * y * (\-0| \-)\.08762[0-9] * 0*\.18830[0-9]" $log | wc -l`
CRIT15=`$GREPL "H * z * 0*\.15176[0-9] * (\-0| \-)\.16825[0-9] * 0*\.38258[0-9]" $log | wc -l`
CRIT16=`$GREPL "H * x * (\-0| \-)\.00126[0-9] * 0*\.03200[0-9] * (\-0| \-)\.00584[0-9] * 0*\.11013[0-9]" $log | wc -l`
CRIT17=`$GREPL "H * y * (\-0| \-)\.02106[0-9] * (\-0| \-)\.01687[0-9] * (\-0| \-)\.01036[0-9] * 0*\.17524[0-9] * 0*\.47972[0-9]" $log | wc -l`
CRIT18=`$GREPL "H * z * 0*\.02479[0-9] * 0*\.04702[0-9] * 0*\.00429[0-9] * (\-0|\-|0| )\.000000 * (\-0|\-|0| )\.000000 * 0*\.09116[0-9]" $log | wc -l`
TEST[6]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6 \+ \
		$CRIT7 \+ $CRIT8 \+ $CRIT9 \+ $CRIT10 \+ $CRIT11 \+ $CRIT12 \+ \
		$CRIT13 \+ $CRIT14 \+ $CRIT15 \+ $CRIT16 \+ $CRIT17 \+ $CRIT18`
CTRL[6]=18
ERROR[6]="INITIAL HESSIAN NOT CORRECT"

# Initial static polarizabilities
CRIT1=`$GREPL "Ex * 1\.394985 * (\-0|\-|0| )\.000000 * (\-0|\-|0| )\.000000" $log | wc -l`
CRIT2=`$GREPL "Ey * (\-0|\-|0| )\.000000 * 5\.719486 * (\-0|\-|0| )\.000000" $log | wc -l`
CRIT3=`$GREPL "Ez * (\-0|\-|0| )\.000000 * (\-0|\-|0| )\.000000 * 5\.719485" $log | wc -l`
TEST[7]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3`
CTRL[7]=3
ERROR[7]="INITIAL STATIC POLARIZABILITIES NOT CORRECT"

# Initial magnetizabilities
CRIT1=`$GREPL "Isotropic magnetizability\: * \-3\.2414 * \-255\.7793 * \-15\.4034" $log | wc -l`
CRIT2=`$GREPL "Diamagnetic contribution\: * \-4\.2616 * \-336\.2815 * \-20*\.2514" $log | wc -l`
CRIT3=`$GREPL "Paramagnetic contribution\: * 1\.0202 * 80*\.5022 * 4\.8480" $log | wc -l`
CRIT4=`$GREPL "Parallel component\: * \-3\.2237 * \-254\.3866 * \-15\.3195" $log | wc -l`
CRIT5=`$GREPL "Perpendicular component\: * \-3\.2502 * \-256\.4757 * \-15\.4453" $log | wc -l`
CRIT6=`$GREPL "Anisotropy\: * 0*\.0265 * 2\.0891 * 0*\.1258" $log | wc -l`
TEST[8]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6`
CTRL[8]=6
ERROR[8]="INITIAL MAGNETIZABILITIES NOT CORRECT"

# Initial g-tensor
CRIT1=`$GREPL "Isotropic g\-tensor  * \: * 0*\.4503330[0-9]" $log | wc -l`
CRIT2=`$GREPL "Nuclear contribution * \: * 0*\.985131(49|50)" $log | wc -l`
CRIT3=`$GREPL "Electronic contribution * \: * (\-0| \-)\.1077988[0-9]" $log | wc -l`
CRIT4=`$GREPL "Parallel component\: * 0*\.4139216[0-9]" $log | wc -l`
CRIT5=`$GREPL "Perpendicular component\: * 0*\.4685387[0-9]" $log | wc -l`
CRIT6=`$GREPL "Anisotropy\: * (\-0| \-)\.0546170[0-9]" $log | wc -l`
TEST[9]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6`
CTRL[9]=6
ERROR[9]="INITIAL ROTATIONAL G-TENSOR NOT CORRECT"

# Initial shielding tensor
CRIT1=`$GREPL "N * x * 290\.4388[0-9]* * (\-0| \-)\.000000[0-9]* * (\-0|\-|0| )\.00000000" $log | wc -l`
CRIT2=`$GREPL "N * y * (\-0| \-)\.00000[0-9][0-9][0-9] * 304\.4396[0-9][0-9][0-9][0-9] * (\-0|\-|0| )\.0000000." $log | wc -l`
CRIT3=`$GREPL "N * z * (\-0|\-|0| )\.00000[0-9][0-9][0-9] * (\-0|\-|0| )\.0000000. * 304\.4396[0-9][0-9][0-9][0-9]" $log | wc -l`
CRIT4=`$GREPL "H * x * 27\.65775[0-9][0-9][0-9] * \-1\.782995[0-9][0-9] * \-3\.088239[0-9][0-9]" $log | wc -l`
CRIT5=`$GREPL "H * y * \-2\.484859[0-9][0-9] * 31\.72638[0-9][0-9][0-9] * 4\.925179[0-9][0-9]" $log | wc -l`
CRIT6=`$GREPL "H * z * \-4\.303904[0-9][0-9] * 4\.925179[0-9][0-9] * 37\.41348[0-9][0-9][0-9]" $log | wc -l`
CRIT7=`$GREPL "H * x * 27\.65775[0-9][0-9][0-9] * \-1\.782995[0-9][0-9] * 3\.088239[0-9][0-9]" $log | wc -l`
CRIT8=`$GREPL "H * y * \-2\.484859[0-9][0-9] * 31\.72638[0-9][0-9][0-9] * \-4\.925179[0-9][0-9]" $log | wc -l`
CRIT9=`$GREPL "H * z * 4\.303904[0-9][0-9] * \-4\.925179[0-9][0-9] * 37\.41348[0-9][0-9][0-9]" $log | wc -l`
CRIT10=`$GREPL "H * x * 27\.65774[0-9][0-9][0-9] * 3\.565990[0-9][0-9] * (\-0|\-|0| )\.00000000" $log | wc -l`
CRIT11=`$GREPL "H * y * 4\.969718[0-9][0-9] * 40\.25703[0-9][0-9][0-9] * (\-0|\-|0| )\.00000000" $log | wc -l`
CRIT12=`$GREPL "H * z * (\-0|\-|0| )\.00000000 * (\-0|\-|0| )\.00000000 * 28\.88282[0-9][0-9][0-9]" $log | wc -l`
# expr 2 + 3 + 3 + 3 + 3 + 3 + 3 + 3 + 3 + 2 + 2 + 2
TEST[10]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6 \+ \
		$CRIT7 \+ $CRIT8 \+ $CRIT9 \+ $CRIT10 \+ $CRIT11 \+ $CRIT12`
CTRL[10]=12
ERROR[10]="INITIAL SHIELDING TENSOR NOT CORRECT"

# Initial chemical shielding for nitrogen
CRIT1=`$GREPL "Shielding constant\: * 299\.7727 ppm" $log | wc -l`
CRIT2=`$GREPL "Anisotropy\: * \-14\.0008 ppm" $log | wc -l`
CRIT3=`$GREPL "S parameter\: * 14\.0008 ppm" $log | wc -l`
TEST[11]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3`
CTRL[11]=3
ERROR[11]="INITIAL CHEMICAL SHIELDING FOR NITROGEN NOT CORRECT"

# Initial chemical shielding for hydrogen
CRIT1=`$GREPL "Shielding constant\: * 32\.2659 ppm" $log | wc -l`
CRIT2=`$GREPL "Anisotropy\: * 13\.9511 ppm" $log | wc -l`
CRIT3=`$GREPL "S parameter\: * 14\.1227 ppm" $log | wc -l`
CRIT4=`$GREPL "A parameter\: * 0*\.7019 ppm" $log | wc -l`
TEST[12]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4`
CTRL[12]=4
ERROR[12]="INITIAL CHEMICAL SHIELDING FOR HYDROGEN NOT CORRECT"

# Summary of initial shieldings
CRIT1=`$GREPL "\@1 N * 299\.7727 * 415\.1852 \-115\.4125 * \-1\.0000 * 14\.0008 * 7\.0004 * 3\.0000" $log | wc -l`
CRIT2=`$GREPL "\@1 H * 32\.2659 * 31\.6749 * 0*\.5910 * 0*\.6669 * 15\.2184 * 13\.9511 * 0*\.2725" $log | wc -l`
TEST[13]=`expr	$CRIT1 \+ $CRIT2`
CTRL[13]=2
ERROR[13]="SUMMARY OF INITIAL SHIELDINGS NOT CORRECT"

# Projected gradient
CRIT1=`$GREPL "Norm of projected gradient * 0*\.018582" $log | wc -l`
TEST[14]=`expr	$CRIT1`
CTRL[14]=1
ERROR[14]="PROJECTED GRADIENT NOT CORRECT"

# Energy at effective geometry
CRIT1=`$GREPL "Energy at effective geometry\: * \-55\.4551193325" $log | wc -l`
TEST[15]=`expr	$CRIT1`
CTRL[15]=1
ERROR[15]="ENERGY AT EFFECTIVE GEOMETRY NOT CORRECT"

# Gradient at effective geometry
CRIT1=`$GREPL "1 * 0*\.006665(49|50) * 0*\.00000019 * -*0*\.00000000 * -0*\.00222180 * 0*\.00488238" $log | wc -l`
CRIT2=`$GREPL "1 * 0*\.00845655 * -0*\.00222180 * 0*\.00488238 * -0*\.00845655 * -0*\.00222189" $log | wc -l`
CRIT3=`$GREPL "1 * -0*\.00976494 * -*0*\.00000000" $log | wc -l`
TEST[16]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3`
CTRL[16]=3
ERROR[16]="GRADIENT AT EFFECTIVE GEOMETRY NOT CORRECT"

# Hessian at effective geometry
CRIT1=`$GREPL "1 * 0*\.3228005[0-9] * (\-0| \-)\.0000002[0-9] * (\-0|\-|0| )\.0000000[0-9] * (\-0| \-)\.1076002[0-9]"  $log | wc -l`
CRIT2=`$GREPL "2 * (\-0| \-)\.0000002[0-9] * 0*\.8175087[0-9] * (\-0|\-|0| )\.0000000[0-9] * 0*\.1207049[0-9]"  $log | wc -l`
CRIT3=`$GREPL "3 * (\-0|\-|0| )\.0000000[0-9] * (\-0|\-|0| )\.0000000[0-9] * 0*\.8175093[0-9] * 0*\.2090671[0-9]"  $log | wc -l`
CRIT4=`$GREPL "4 * (\-0| \-)\.1076002[0-9] * 0*\.1207049[0-9] * 0*\.2090671[0-9] * 0*\.1071549[0-9]"  $log | wc -l`
CRIT5=`$GREPL "5 * 0*\.0665559[0-9] * (\-0| \-)\.1859448[0-9] * (\-0| \-)\.1499231[0-9] * (\-0| \-)\.0879808[0-9]"  $log | wc -l`
CRIT6=`$GREPL "6 * 0*\.1152783[0-9] * (\-0| \-)\.1499232[0-9] * (\-0| \-)\.3590613[0-9] * (\-0| \-)\.1523873[0-9]"  $log | wc -l`
CRIT7=`$GREPL "7 * (\-0| \-)\.1076002[0-9] * 0*\.1207049[0-9] * (\-0| \-)\.2090671[0-9] * 0*\.0002226[0-9]"  $log | wc -l`
CRIT8=`$GREPL "8 * 0*\.0665559[0-9] * (\-0| \-)\.1859448[0-9] * 0*\.1499231[0-9] * (\-0| \-)\.0112991[0-9]"  $log | wc -l`
CRIT9=`$GREPL "9 * (\-0| \-)\.1152783[0-9] * 0*\.1499232[0-9] * (\-0| \-)\.3590613[0-9] * (\-0| \-)\.0312629[0-9]"  $log | wc -l`
CRIT10=`$GREPL "10 * (\-0| \-)\.1076000[0-9] * (\-0| \-)\.2414096[0-9] * (\-0|\-|0| )\.0000000[0-9] * 0*\.0002226[0-9]"  $log | wc -l`
