CRIT6=`$GREPL "CY * (0| |\-0|\-)\.0000000[0-9] * (\-0|\-|0| )\.00000[0-9][0-9][0-9] * (\-0|\-|0| )\.00000[0-9][0-9][0-9]" $log | wc -l`
CRIT7=`$GREPL "AZ * (\-0|\-|0| )\.0000000[0-9] * (\-0|\-|0| )\.0000[0-7][0-9][0-9][0-9] * (\-0|\-|0| )\.0000[0-7][0-9][0-9][0-9]" $log | wc -l`
CRIT8=`$GREPL "BZ * (0| |\-0|\-)\.0000000[0-9] * (\-0|\-|0| )\.00000[0-9][0-9][0-9] * (\-0|\-|0| )\.00000[0-9][0-9][0-9]" $log | wc -l`
CRIT9=`$GREPL "CZ * 0*\.4685386[0-9] * (\-0| \-)\.0036[2-5]..[0-9] * 0*\.464[8-9]...[0-9]" $log | wc -l`
TEST[35]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5 \+ $CRIT6 \+ \
		$CRIT7 \+ $CRIT8 \+ $CRIT9`
CTRL[35]=9
ERROR[35]="VIBRATIONALLY AVERAGED G-TENSOR NOT CORRECT"

# Vibrationally averaged nuclear magnetic shielding constants for nitrogen
CRIT1=`$GREPL "Bxmx * 290\.4388...[0-9] * 0*\.66[0-7]....[0-9] * 291\.(098|099|100|101|102|103|104|105|106|107)....[0-9]" $log | wc -l`
CRIT2=`$GREPL "Bymx * 0*\.000000.[0-9] * (\-0|\-|0| )\.00[0-8]....[0-9] * (\-0|\-|0| )\.00[0-8]....[0-9]" $log | wc -l`
CRIT3=`$GREPL "Bxmy * (\-0|\-)\.000000.[0-9] * (\-0|\-|0| )\.00[0-9]....[0-9] * (\-0|\-|0| )\.00[0-9]....[0-9]" $log | wc -l`
CRIT4=`$GREPL "Bymy * 304\.4396...[0-9] * \-2\.05[7-9]....[0-9] * 302\.38[0-3]....[0-9]" $log | wc -l`
CRIT5=`$GREPL "Bzmz * 304\.4396...[0-9] * \-2\.0[5-6].....[0-9] * 302\.3[7-8].....[0-9]" $log | wc -l`
TEST[36]=`expr	$CRIT1 \+ $CRIT2 \+ $CRIT3 \+ $CRIT4 \+ $CRIT5`
CTRL[36]=5
