#INCLUDES       =  
#LIBS           =
#AR             = ar
#ARFLAGS        = rvs
##
#
# IFORT EXAMPLE
#
#ARCH        = linux
#FMMDIR      = mm
#
#
#CPPFLAGS      = -DVAR_ICHOR
#F90            = ifort
#FLNK           = ifort
#RM             = rm -f
#F90OPTFLAGS    = -O3  -ip -w -fpp1 -openmp
##F90OPTFLAGS    = -g -O0 -ip -w -fpp1 -check all #-openmp
#INCLUDES       =  
#LIBS           = #-openmp
#AR             = ar
#ARFLAGS        =  rvs
#
# PGI SISYPHOS EXAMPLE
#
#ARCH        = linux
#FMMDIR      = mm
#
#
#CPPFLAGS      = -DVAR_ICHOR -DVAR_OMP -DVAR_OPENACC -DVAR_PGF90
#F90            = pgf90
#FLNK           = pgf90
#RM             = rm -f
#F90OPTFLAGS    = -mp -Mconcur -acc -Minfo=accel
#INCLUDES       =  
#LIBS           = 
#AR             = ar
#ARFLAGS        =  rvs
#
#########################################################
#
# END OF COMPILER SECTION 
#
#########################################################

